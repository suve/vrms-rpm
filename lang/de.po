msgid "msg_help_usage"
msgstr "Verwendung: vrms-rpm [Optionen]"
msgid "msg_help_option_ascii"    
msgstr "    Zeigt rms ASCII-Kunst an wenn keine kostenpflichtigen Pakete gefunden wurden,\n"
       "    oder wenn mehr als 10%% der Pakete kostenpflichtig sind."
msgid "msg_help_option_explain"
msgstr "    Zeigt beim Auflisten der Pakete die Lizenzen, um die\n"
       "    kostenlosen / -pflichtigen Klassifikationen zu begründen"
msgid "msg_help_option_help"
msgstr "    Zeigt diese Hilfe an und beendet das Programm."
msgid "msg_help_option_list"
msgstr "    Abgesehen von einer Summe der kostenlosen & -pflichtigen Pakete,\n"
       "    werden dessen Namen aufgelistet. Der Standardwert ist 'kostenpflichtig'"
msgid "msg_help_option_version"
msgstr "    Zeigt die Version an und beendet das Programm."
msgid "msg_list_noargument"
msgstr "%s: --list Option benötigt ein Argument"
msgid "msg_list_badargument"
msgstr "%s: Argument für die --list Option muss einer der folgenden Werte sein: 'none', 'free', 'nonfree', 'all'"
msgid "msg_unknown_option"
msgstr "%s: unbekannte Option '%s'"
msgid "msg_total_free"
msgstr "%d kostenlose Pakete"
msgid "msg_total_nonfree"
msgstr "%d kostenpflichtige Pakete"
msgid "msg_rms_happy_ascii"
msgstr "Nur kostenlose Pakete - rms würde stolz sein!                           rms ist glücklich."
msgid "msg_rms_happy"
msgstr "Nur kostenlose Pakete - rms würde stolz sein!"
msgid "msg_rms_disappoint_ascii"
msgstr "Mehr als 10%% kostenpflichtige Pakete. Hasst du Freiheit?              rms ist enttäuscht."
msgid "msg_rms_disappoint"
msgstr "Mehr als 10%% kostenpflichtige Pakete. Hasst du Freiheit?"
