# Et sted til personlig anerkendelse!
# Skriv noget lignende "Dansk oversættelse af Jvlivs Gaivs\n" her.
msgid "TRANSLATION_AUTHOR\n"
msgstr "--\n"

msgid "FREE_PACKAGES_COUNT\n"
msgid_plural "FREE_PACKAGES_COUNT\n"
msgstr[0] "%d fri pakke\n"
msgstr[1] "%d frie pakker\n"

msgid "NONFREE_PACKAGES_COUNT\n"
msgid_plural "NONFREE_PACKAGES_COUNT\n"
msgstr[0] "%d ikke-fri pakke\n"
msgstr[1] "%d ikke-frie pakker\n"

msgid "RMS_HAPPY\n"
msgstr "Kun frie pakker - rms er stolt!\n"

msgid "RMS_DISAPPOINTED\n"
msgstr "Over 10%% ikke-fri software-pakker. Kan du ikke lide frihed?\n"

msgid "HELP_USAGE\n"
msgstr "Usage: vrms-rpm [valgmuligheder]\n"

msgid "HELP_OPTION_ASCII\n"
msgstr "    Vis rms ASCII-kunst når ingen ikke-fri pakker er fundet,\n"
       "    eller når ikke-fri pakker er 10%% eller mere af det totale."

msgid "HELP_OPTION_COLOUR\n"
msgstr "    Styrrer om terminal escape sekvens skal bruges\n"
       "    til at farve output. Standardinstillingen er 'auto', som bruger farvet output\n"
       "    når der bliver skrevet i terminalen, men ikke når der bliver skrevet til en fil eller pipe.\n"

msgid "HELP_OPTION_DESCRIBE\n"
msgstr "    Inkluder pakkereferat i pakkeoversigten (kort beskrivelse).\n"

msgid "HELP_OPTION_EXPLAIN\n"
msgstr "    Vis licenser i pakkeoversigten, for at fremhæve\n"
       "    fri / ikke-fri klassificering."

msgid "HELP_OPTION_HELP\n"
msgstr "    Vis denne hjælp og exit.\n"

msgid "HELP_OPTION_IMAGE\n"
msgstr "    Lignende --ascii, men vis et billede af Unicode block karakterer\n"
       "    og 256-farve escape koder.\n"

msgid "HELP_OPTION_LICENCELIST\n"
msgstr "    Specificer listen af gode licenser. FILE kan være en sti\n"
       "    til en fil på disken, eller en af licenserne i listen:\n"
       "    %s\n"
       "    Standardinstillingen er '%s'.\n"

msgid "HELP_OPTION_LIST\n"
msgstr "    Vis ikke kun antal fri / ikke-fri pakker, men vis\n"
       "    også navn. Standardinstillingen er 'nonfree'."

msgid "HELP_OPTION_VERSION\n"
msgstr "    Vis information om version og exit."

msgid "ERR_PIPE_OPEN_FAILED\n"
msgstr "vrms-rpm: det lykkedes ikke at åbne pipe til /usr/bin/rpm\n"

msgid "ERR_PIPE_NOEVENTS\n"
msgstr "vrms-rpm: pipe_fopen(): poll() returnerede ingen events\n"

msgid "ERR_PIPE_POLL_ERROR\n"
msgstr "vrms-rpm: pipe_fopen(): poll() returnerede en fejl\n"

msgid "ERR_PIPE_POLL_HANGUP\n"
msgstr "vrms-rpm: pipe_fopen(): den anden ende af pipe afsluttede uden at sende noget data\n"

msgid "ERR_PIPE_READ_FAILED\n"
msgstr "vrms-rpm: det lykkedes ikke at læse fra pipe til /usr/bin/rpm\n"

msgid "ERR_LICENCES_FAILED\n"
msgstr "vrms-rpm: det lykkedes ikke at læse listen af gode licenser\n"

msgid "ERR_LICENCES_BADFILE\n"
msgstr "vrms-rpm: det lykkedes ikke at læse listen af gode licenser fra \"%s\"\n"

msgid "ERR_BADOPT_COLOUR\n"
msgstr "vrms-rpm: argumentet til --colour valgmuligheden skal være en af 'no', 'yes', eller 'auto'\n"

msgid "ERR_BADOPT_LIST\n"
msgstr "vrms-rpm: argumentet til --list valgmuligheden skal være en af 'none', 'non-free', 'free' eller 'all'\n"

msgid "ERR_BADOPT_UNKNOWN\n"
msgstr "vrms-rpm: ukendt valgmulighed \"%s\"\n"